---
import Base from "@/layouts/base.astro";
import Wall from "@/components/wall.tsx";

import { randosort } from "@/utils";
import { getCollection } from "astro:content";

const names = await getCollection("names");
---

<Base headline="Names" color="red-400" href="/" title="">
  <Wall names={names.sort(randosort).map((n) => [n.id, n.data.name])} />
</Base>
